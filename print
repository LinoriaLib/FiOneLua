local StarterGui = game:GetService("StarterGui")

-- Create a ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "VoidScriptBuilderUI"
screenGui.Parent = StarterGui

-- Create a Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.3, 0, 0.3, 0) -- 30% width and height of the screen
frame.Position = UDim2.new(0.35, 0, 0.35, 0) -- Center the frame
frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
frame.Parent = screenGui

-- Create a TextLabel
local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(1, 0, 0.2, 0) -- Full width, 20% height of the frame
textLabel.Position = UDim2.new(0, 0, 0, 0)
textLabel.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
textLabel.Text = "Void Script Builder"
textLabel.Font = Enum.Font.SourceSans
textLabel.TextSize = 24
textLabel.Parent = frame
